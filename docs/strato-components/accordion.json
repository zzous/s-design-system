{
  "SAccordionMenu": {
    "description": "아코디언 스타일의 접을 수 있는 메뉴 컴포넌트",
    "usage": "접었다 펼 수 있는 메뉴나 콘텐츠 영역을 구현할 때 사용",
    "props": {
      "title": {
        "type": "String",
        "required": true,
        "default": null,
        "description": "아코디언 메뉴의 제목"
      },
      "titleBold": {
        "type": "Boolean",
        "required": false,
        "default": false,
        "description": "제목을 굵게 표시할지 여부"
      }
    },
    "slots": {
      "default": "아코디언 내부에 표시될 콘텐츠"
    },
    "example": {
      "import": [
        "import { SAccordionMenu } from '@'"
      ],
      "template": "<SAccordionMenu title=\"메뉴 제목\" :titleBold=\"true\"><div>아코디언 내부 콘텐츠</div></SAccordionMenu>"
    }
  }
}
