{
  "SFormTable": {
    "description": "폼 레이아웃을 구성하기 위한 테이블 컴포넌트",
    "usage": "폼 필드들을 테이블 형태로 정렬하여 표시",
    "slots": {
      "default": "SFormItem 컴포넌트들이 위치하는 영역"
    }
  },
  "SFormItem": {
    "description": "폼 테이블 내부에서 사용되는 개별 입력 필드 컴포넌트",
    "props": {
      "label": {
        "type": "String",
        "required": true,
        "default": "",
        "description": "폼 라벨"
      },
      "required": {
        "type": "Boolean",
        "required": false,
        "default": false,
        "description": "필수값 여부"
      },
      "showLabel": {
        "type": "Boolean",
        "required": false,
        "default": true,
        "description": "폼라벨 노출 여부"
      },
      "className": {
        "type": "String",
        "required": false,
        "default": "",
        "description": "페이지타이틀"
      },
      "contentClass": {
        "type": "String",
        "required": false,
        "default": "",
        "description": "콘텐트 영역 클래스"
      },
      "name": {
        "type": "String",
        "required": false,
        "default": "",
        "description": "validation 체크 시 필수 값"
      }
    },
    "slots": {
      "default": "메인 입력 필드 영역",
      "outer-append": "입력 필드 우측에 추가 요소 배치"
    },
    "example": {
      "import": [
        "import { SFormTable, SFormItem } from '@'",
        "import { VTextField } from 'vuetify/components'",
        "import { SBtn } from '@'"
      ],
      "template": "<SFormItem label=\"이름\" required><VTextField placeholder=\"이름을 입력하세요\" variant=\"outlined\" density=\"compact\" hide-details=\"auto\" /><template #outer-append><SBtn color=\"black\" size=\"small\" title=\"중복 체크\" /></template></SFormItem>"
    }
  }
}
