{
  "SFileInputBtn": {
    "description": "파일 선택을 위한 버튼이 포함된 파일 입력 컴포넌트",
    "usage": "파일 업로드가 필요한 폼에서 사용",
    "props": {
      "placeholder": {
        "type": "String",
        "required": false,
        "default": "파일을 선택하세요",
        "description": "입력 필드의 플레이스홀더"
      },
      "modelValue": {
        "type": ["Object", "String"],
        "required": false,
        "default": null,
        "description": "선택된 파일 객체"
      },
      "buttonText": {
        "type": "String",
        "required": false,
        "default": "파일선택",
        "description": "버튼에 표시될 텍스트"
      },
      "accept": {
        "type": "String",
        "required": false,
        "default": "*",
        "description": "허용되는 파일 타입"
      }
    },
    "emits": ["update:model-value"],
    "example": {
      "import": ["import { SFileInputBtn } from '@'"],
      "template": "<SFileInputBtn v-model=\"file\" accept=\".jpg,.png\" button-text=\"이미지 선택\" />"
    }
  },
  "SShellScriptInput": {
    "description": "쉘 스크립트 입력을 위한 텍스트 에디터 컴포넌트",
    "usage": "쉘 스크립트 작성이 필요한 곳에서 사용",
    "props": {
      "modelValue": {
        "type": "String",
        "required": true,
        "default": "",
        "description": "쉘 스크립트 내용"
      },
      "labelTitle": {
        "type": "String",
        "required": false,
        "default": "",
        "description": "입력 필드 상단에 표시될 라벨"
      }
    },
    "features": ["라인 번호 자동 표시", "자동 높이 조절"]
  }
}
