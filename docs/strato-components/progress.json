{
  "SGlobalProgress": {
    "description": "전역 로딩 바 컴포넌트",
    "usage": "전체 화면에 대한 로딩 상태를 표시하는 오버레이 컴포넌트",
    "props": {
      "loadingMessage": {
        "type": "String",
        "required": false,
        "default": "Check-loading",
        "description": "로딩창 메세지 값"
      },
      "modelValue": {
        "type": "Boolean",
        "required": false,
        "default": false,
        "description": "로딩 오픈 여부"
      },
      "teleportId": {
        "type": "String",
        "required": false,
        "default": "destination",
        "description": "Teleport ID"
      }
    }
  },
  "SProgressCircular": {
    "description": "원형 프로그레스 컴포넌트",
    "usage": "로딩 상태를 표시하는 애니메이션 원형 프로그레스",
    "props": {
      "size": {
        "type": "Number",
        "required": false,
        "default": 50,
        "description": "프로그레스 크기 (px)"
      },
      "width": {
        "type": "Number",
        "required": false,
        "default": 6,
        "description": "프로그레스 선 두께 (px)"
      }
    },
    "features": {
      "animations": [
        "회전 애니메이션",
        "그라데이션 효과 (파란색 #1B75BB에서 보라색 #7C0F80)",
        "부드러운 트랜지션"
      ]
    }
  }
}
