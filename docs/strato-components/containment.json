{
  "SEmpty": {
    "description": "빈 상태나 데이터가 없는 상태를 표시하는 컨테이너 컴포넌트",
    "usage": "데이터가 없거나 빈 상태를 사용자에게 표시할 때 사용",
    "props": {
      "title": {
        "type": "String",
        "required": false,
        "default": "",
        "description": "empty 제목"
      },
      "description": {
        "type": "String",
        "required": false,
        "default": "",
        "description": "empty 내용"
      },
      "elevation": {
        "type": ["Number", "String"],
        "required": false,
        "default": "undefined",
        "description": "카드의 입체감 조절 0~24까지 가능"
      },
      "outlined": {
        "type": "Boolean",
        "required": false,
        "default": "true",
        "description": "카드의 외곽선 설정"
      },
      "shadowed": {
        "type": "Boolean",
        "required": false,
        "default": "true",
        "description": "카드에 그림자 효과 부여 on/off"
      }
    },
    "slots": {
      "default": "카드 내부에 추가할 커스텀 콘텐츠"
    },
    "example": {
      "import": ["import { SEmpty } from '@'"],
      "template": "<SEmpty title=\"데이터 없음\" description=\"표시할 데이터가 없습니다.\" :elevation=\"2\" />"
    }
  }
}
