{
  "SCardTable": {
    "description": "카드 형태의 테이블 컴포넌트",
    "usage": "데이터를 카드 형태로 표시하는 테이블",
    "props": {
      "itemValue": {
        "type": "String",
        "required": false,
        "default": "id",
        "description": "선택된 아이템의 value 값"
      },
      "itemTitle": {
        "type": "String",
        "required": false,
        "default": "undefined",
        "description": "title이 될 item 값"
      },
      "datas": {
        "type": "Array",
        "required": false,
        "default": "[]",
        "description": "테이블에 표시될 데이터 배열"
      },
      "headers": {
        "type": "Array",
        "required": false,
        "default": "[]",
        "description": "key, title 값으로 구성된 헤더 배열"
      },
      "itemPerPage": {
        "type": ["Number", "String"],
        "required": false,
        "default": "5",
        "description": "페이지당 표시할 아이템 수"
      },
      "search": {
        "type": ["String", "Array"],
        "required": false,
        "default": "undefined",
        "description": "검색어 또는 key:value Object를 포함한 검색 배열"
      },
      "showSelect": {
        "type": "Boolean",
        "required": false,
        "default": false,
        "description": "선택 체크박스 표시 여부"
      }
    },
    "events": {
      "update:page": "페이지 변경 시 발생",
      "update:selected": "아이템 선택 시 발생"
    },
    "slots": {
      "table-select": "선택 체크박스 커스텀 슬롯",
      "table-expand": "확장 영역 커스텀 슬롯",
      "expanded-row": "확장된 행 컨텐츠 슬롯",
      "empty-content": "데이터가 없을 때 표시될 컨텐츠"
    }
  },
  "SDataTable": {
    "description": "기본 데이터 테이블 컴포넌트",
    "usage": "페이지네이션과 정렬 기능을 포함한 데이터 테이블",
    "props": {
      "headers": {
        "type": "Array",
        "required": false,
        "default": "[]",
        "description": "테이블 헤더 정보"
      },
      "items": {
        "type": "Array",
        "required": false,
        "default": "[]",
        "description": "테이블 데이터"
      },
      "itemsPerPage": {
        "type": ["String", "Number"],
        "required": false,
        "default": "10",
        "description": "페이지당 표시할 아이템 수"
      },
      "search": {
        "type": "String",
        "required": false,
        "default": "undefined",
        "description": "검색어"
      },
      "smartSearch": {
        "type": "Array",
        "required": false,
        "default": "[]",
        "description": "스마트 검색 옵션 배열"
      },
      "density": {
        "type": "String",
        "required": false,
        "default": "default",
        "description": "테이블 행 높이 (default | comfortable | compact)"
      }
    },
    "events": {
      "update:page": "페이지 변경 시 발생",
      "update:sort-by": "정렬 변경 시 발생",
      "update:model-value": "선택 항목 변경 시 발생"
    }
  }
}
